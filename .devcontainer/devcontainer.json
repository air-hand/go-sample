{
  "name": "Remote debug",
  "dockerComposeFile": [
    "../docker-compose.yml",
    "./docker-compose.extend.yml",
  ],
  "service": "web",
  "workspaceFolder": "/opt/app/",
  "extensions": [
    "golang.go",
    "editorconfig.editorconfig",
  ],
  "settings": {
    "gopls": {
      "build.experimentalWorkspaceModule": true
    }
  },
  "shutdownAction": "stopCompose",
}